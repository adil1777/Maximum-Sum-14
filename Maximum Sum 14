#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
       int n,i,j,s=0,max;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    max=0;
     for(i=0;i<n;i++){
             s=s+a[i];
             if(s>max){
                 max=s;
         }
         if(s<0){
             s=0;
         }
     }
    printf("%d",max);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
